<html>
<head>
	<title>$tonks - Home</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- Bootstrap -->
	<!-- This is version 3.4.1! -->
	<link href="css/stonks.css" rel="stylesheet">
	<link href="css/bootstrap.css" rel="stylesheet">
	<link href="css/boostrap-theme.css" rel="stylesheet">
	<!-- jQuery -->
	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="js/jquery-1.11.0.js"></script>
	<script src="js/bootstrap.js"></script>
</head>
<!-- Main Page -->
<body>
	<!-- Heading -->
	<div class="container-fluid">
		<div class="jumbotron">
			<h1 class="stonks">$tonks</h1>
			<p class="stonks">Taking You to the Moon<br>
				 								One Lesson at a Time<br>
												Or Whatever Works Best For You</p>
		</div>
	</div>
	<!-- Heading End -->

	<!-- Dashboard -->
	<div class="panel-group">
		<div class="panel panel-default" id="Intro">
			<div class="panel-heading">
				<h2>Welcome Back!</h2>
			</div>
			<div class="panel-body">
				<p>Welcome back Scott Doe. Let us get you started. </p>
			</div>
		</div>
		<!-- Review Previous Lessons -->
		<!-- This is Panel 1! -->
		<div class="panel panel-default" id="Review">
			<!-- Heading -->
			<div class="panel-heading">
				<h2>Previous Lessons</h2>
			</div>
			<!-- Body -->
			<div class="panel-body">
				<!-- Description -->
				<p>Here are the lessons you've completed!</p>
				<!-- Carousel -->
				<div id="previous-lessons" class="carousel slide" data-ride="carousel">
  				<!-- Indicators -->
					<!-- There are 3 slides per carousel! -->
  				<ol class="carousel-indicators">
    				<li data-target="#previous-lessons" data-slide-to="0" class="active"></li>
    				<li data-target="#previous-lessons" data-slide-to="1"></li>
    				<li data-target="#previous-lessons" data-slide-to="2"></li>
  				</ol>
					<!-- Wrapper for slides -->
  				<div class="carousel-inner">
    				<div class="item active">
      				<img src="..." alt="...">
      				<div class="carousel-caption">
        				...
      				</div>
    				</div>
    				<div class="item">
      				<img src="..." alt="...">
      				<div class="carousel-caption">
        				...
      				</div>
    				</div>
    					...
  				</div>
  				<!-- Carousel Controls -->
					<!-- The button format is not important right now! -->
  				<a class="left carousel-control" href="#previous-lessons" role="button" data-slide="prev">
    				<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    				<span class="sr-only">Previous</span>
  				</a>
  				<a class="right carousel-control" href="#previous-lessons" role="button" data-slide="next">
    				<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    				<span class="sr-only">Next</span>
  				</a>
				</div>
			</div>
		</div>
		<!-- End of Panel 1 -->

		<!-- Next Lessons -->
		<!-- This is Panel 2! -->
		<div class="panel panel-default" id="Preview">
			<!-- Heading -->
			<div class="panel-heading">
				<h2>Resume Lessons</h2>
			</div>
			<!-- Body -->
			<div class="panel-body">
				<!-- Description -->
				<p>Here are lessons you haven't completed!</p>
				<!-- Carousel -->
				<div id="next-lessons" class="carousel slide" data-ride="carousel">
  				<!-- Indicators -->
					<!-- There are 3 slides per carousel! -->
  				<ol class="carousel-indicators">
    				<li data-target="#next-lessons" data-slide-to="0" class="active"></li>
    				<li data-target="#next-lessons" data-slide-to="1"></li>
    				<li data-target="#next-lessons" data-slide-to="2"></li>
  				</ol>
					<!-- Wrapper for slides -->
  				<div class="carousel-inner">
    				<div class="item active">
      				<img src="..." alt="...">
      				<div class="carousel-caption">
        				...
      				</div>
    				</div>
						<!-- The remaining .item elements can be turned into a handlebars loop! -->
						<!-- See the template I e-mailed -->
    				<div class="item">
      				<img src="..." alt="...">
      				<div class="carousel-caption">
        				...
      				</div>
    				</div>
    					...
  				</div>
  				<!-- Carousel Controls -->
					<!-- The button format is not important right now! -->
  				<a class="left carousel-control" href="#next-lessons" role="button" data-slide="prev">
    				<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    				<span class="sr-only">Previous</span>
  				</a>
  				<a class="right carousel-control" href="#next-lessons" role="button" data-slide="next">
    				<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    				<span class="sr-only">Next</span>
  				</a>
				</div>
			</div>
		</div>
		<!-- End of Panel 2 -->

		<!-- Recent News About Investing -->
		<!-- This is Panel 3! -->
		<div class="panel panel-default" id="News">
			<!-- Heading -->
			<div class="panel-heading">
				<h2>Changes in Investing</h2>
			</div>
			<!-- Body -->
			<div class="panel-body">
				<!-- Description -->
				<p>The world of investing is always changing!</p>
				<!-- Carousel -->
				<div id="investing-news" class="carousel slide" data-ride="carousel">
  				<!-- Indicators -->
					<!-- There are 3 slides per carousel! -->
  				<ol class="carousel-indicators">
    				<li data-target="#investing-news" data-slide-to="0" class="active"></li>
    				<li data-target="#investing-news" data-slide-to="1"></li>
    				<li data-target="#investing-news" data-slide-to="2"></li>
  				</ol>
					<!-- Wrapper for slides -->
  				<div class="carousel-inner">
    				<div class="item active">
      				<img src="..." alt="...">
      				<div class="carousel-caption">
        				...
      				</div>
    				</div>
						<!-- The remaining .item elements can be turned into a handlebars loop! -->
						<!-- See the template I e-mailed -->
    				<div class="item">
      				<img src="..." alt="...">
      				<div class="carousel-caption">
        				...
      				</div>
    				</div>
    					...
  				</div>
  				<!-- Carousel Controls -->
					<!-- The button format is not important right now! -->
  				<a class="left carousel-control" href="#investing-news" role="button" data-slide="prev">
    				<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    				<span class="sr-only">Previous</span>
  				</a>
  				<a class="right carousel-control" href="#investing-news" role="button" data-slide="next">
    				<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    				<span class="sr-only">Next</span>
  				</a>
				</div>
			</div>
		</div>
		<!-- End of Panel 3 -->
	</div>
	<!-- End of Dashboard -->

	<!-- Navbar -->
	<nav class="navbar navbar-fixed-bottom">
		<div class="container-fluid">
			<ul class="nav nav-tabs nav-justified">
				<!-- Active denotes what page a user is currently on! -->
				<!-- The list below may be a handlebars loop! -->
				<li class="active">
    			<a href="/home">Home</a>
  			</li>
				<li>
    			<a href="/education">Education</a>
  			</li>
				<li>
    			<a href="/investing">Investing</a>
